/* pages/movies/movie.wxss */
.wrapper {
  background: #efeff4;
  width: 100%;
  box-sizing: border-box;
  padding-top: 2%;
  padding-bottom: 2%;
}
.med {
  width: 100%;
  box-sizing: border-box;
  margin-top:20rpx;
  background-color: #fff;
  padding-left: 20rpx;
}
.med text{
  width: 100%;
  height: 80rpx;
  color: #000;
  font-size: 30rpx;
}
.med view{
  width: 100%;
}

.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}