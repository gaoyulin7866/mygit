/* pages/movies/movie.wxss */
.wrapper {
  background: #efeff4;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding-left: 1%;
  padding-top: 20rpx;
}
.wrapper-move video{
  width: 100%;
}
.wrapper-move {
  display: inline-block;
  width: 49%;
  height: 300rpx;
  margin-right: 1%;
}
.wrapper-move text{
  display: block;
  width: 100%;
  height: 60rpx;
  font-size: 40rpx;
  text-align: center;
}